http://example.org/feed1.xml
http://example.org/feed2.xml < set reload-period 60
http://example.org/feed3.xml "Feed 3 title"
http://example.org/feed4.xml "Feed 4 title" < set reload-period 120
http://example.org/feed5.xml < set reload-period 180; set item-limit 100
http://example.org/feed6.xml  "Feed 6 title"    <     set    reload-period   111   ;    set    item-limit  222
http://example.org/feed7.xml     <     set    reload-period   333   ;;;  ; ; ;;;;set    item-limit  444
http://example.org/feed8.xml < set proxy 127.0.0.1
http://example.org/feed9.xml < set proxy "localhost"
$(curl foo://bar.baz/feed10) < set proxy 'localhost.localdomain'
$(curl foo://bar.baz/feed11) < set proxy 127.0.0.1;
$(curl foo://bar.baz/feed12) < set proxy 127.0.0.1;;
$(curl foo://bar.baz/feed13) < set proxy "127.0.0.1";
$(curl foo://bar.baz/feed14) < set proxy '127.0.0.1';
$(curl foo://bar.baz/feed15) < set proxy "127.0.0.1";;
$(curl foo://bar.baz/feed16) < set proxy '127.0.0.1';;
$(curl foo://bar.baz/feed17) [60] {100}
$(curl foo://bar.baz/feed18) [60] {100} < set reload-period 120; set item-limit 200
